{
  "name": "letter",
  "version": "0.0.16",
  "description": "",
  "author": "zhongpei <1000@intry.cn>",
  "email": "1000@intry.cn",
  "website": "https://iczp.net",
  "private": true,
  "license": "MIT",
  "keywords": [
    "nestjs",
    "edgedb"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iczp/nestjs-letter.git"
  },
  "scripts": {
    "build": "nest build",
    "build:dc": "docker-compose up --build -d",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "ejs": "npx @edgedb/generate edgeql-js && npx @edgedb/generate interfaces",
    "em": "edgedb migrate",
    "emc": "edgedb migration create",
    "start": "nest start",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "plop": "npx plopfile.js",
    "postversion": "ts-node scripts/update-docker-compose-version.js",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.0",
    "@swc/helpers": "^0.5.13",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.7.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "edgedb": "^1.5.10",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@edgedb/generate": "^0.5.5",
    "@nestjs/cli": "^10.4.4",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.23",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.16.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "jest": "^29.5.0",
    "plop": "^4.0.1",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "yaml": "^2.5.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
